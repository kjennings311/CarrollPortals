@{
    ViewBag.Title = "Portfolio List";
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Properties</h2>
        <ol class="breadcrumb">
            <li>
                <a href="index.html">Home / Forms</a>
            </li>
           
            <li class="active">
                <strong>General Liability Claim</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>


<div class="RowsContatiner animated fadeInRight">
    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Equity Partners <small></small></h5>
                <div class="ibox-tools">
                    @*<a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>*@
                    <a disabled="disabled" class="btnEdit" href="#" onclick="ToggleEdit('generalliability');"><i class="fa fa-edit"></i> Edit</a>

                    <a disabled="disabled" class="btnDelete" href="#">
                        <i class="fa fa-trash"></i> Delete
                    </a>

                    <a style="color:#1ab394;" href="#" onclick="ToggleAdd('partners');">
                        <i class="fa fa-plus-circle"></i> Add New
                    </a>


                </div>
            </div>
            <div class="ibox-content">
                <table class="table table-striped table-bordered table-hover dtprops">
                    <thead>
                        <tr>
                            @*<th></th>*@
                            <th>Property</th>
                            <th>Incident Date</th>
                            <th>Incident Location</th>
                            <th>Incident Description</th>
                            <th>Authorities Contacted</th>
                            <th>Contact Person</th>
                            <th>Claimant Name</th>
                            <th>Claimant Address</th>
                            <th>Claimant Phone1</th>
                            <th>Claimant Phone2</th>
                            <th>Any Injuries?</th>
                            <th>Injury Description</th>
                            <th>Witness Present</th>
                            <th>Witness Name</th>
                            <th>Witness Phone</th>
                            <th>Witnness Address</th>
                            <th>Description Of Property</th>
                            <th>Description Of Damage</th>
                            <th>Reported By</th>
                            <th>Reported Date</th>
                            <th>Reported Phone</th>
                            <th>Created By Name</th>
                            <th>Created By</th>

                        </tr>
                    </thead>

                    <tfoot>
                        <tr>
                            @*<th></th>*@
                            <th>Property</th>
                            <th>Incident Date</th>
                            <th>Incident Location</th>
                            <th>Incident Description</th>
                            <th>Authorities Contacted</th>
                            <th>Contact Person</th>
                            <th>Claimant Name</th>
                            <th>Claimant Address</th>
                            <th>Claimant Phone1</th>
                            <th>Claimant Phone2</th>
                            <th>Any Injuries?</th>
                            <th>Injury Description</th>
                            <th>Witness Present</th>
                            <th>Witness Name</th>
                            <th>Witness Phone</th>
                            <th>Witnness Address</th>
                            <th>Description Of Property</th>
                            <th>Description Of Damage</th>
                            <th>Reported By</th>
                            <th>Reported Date</th>
                            <th>Reported Phone</th>
                            <th>Created By Name</th>
                            <th>Created By</th>

                        </tr>
                    </tfoot>
                </table>
            </div>
        </div>
    </div>

</div>
<div class="AddEditContainer row" style="display:none;">

    <div class="col-lg-12">
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h5>Add/Edit Partner</h5>
                <div class="ibox-tools">
                    @*<a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>*@
                    <a class="warning" href="#" onclick="ToggleAdd('generalliability');">
                        <i class="fa fa-minus-circle"></i> Cancel
                    </a>
                    @*<ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#" >Cancel</a>
                            </li>

                        </ul>*@
                    @*<a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>*@
                </div>
            </div>
            <div class="ibox-content dynamicForm">

            </div>
        </div>
    </div>

</div>

@if (ViewBag.ErrorMessage == "AuthorizationRequired")
            {
    <p>You have to sign-in to see your to do list. Click @Html.ActionLink("here", "Index", "Form", new { reauth = true }, null) to sign-in.</p>

}
@if (ViewBag.ErrorMessage == "UnexpectedError")
            {
    <p>An unexpected error occurred while retrieving your to do list.  Please try again.  You may need to sign-in.</p>
}

