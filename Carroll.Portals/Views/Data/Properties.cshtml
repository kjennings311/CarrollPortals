                <div class="ibox-tools">
                    @*<a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>*@
                    <a disabled="disabled" class="btnEdit" href="#" onclick="ToggleEdit('property');"><i class="fa fa-edit"></i> Edit</a>

                    <a disabled="disabled" class="btnDelete" href="#">
                        <i class="fa fa-trash"></i> Delete
                    </a>

                    <a style="color:#1ab394;" href="#"  onclick="ToggleAdd('property');">
                        <i class="fa fa-plus-circle"></i> Add New
                    </a>
                  
                       
                </div>
            </div>
            <div class="ibox-content">
                @*<table class="table table-striped table-bordered table-hover dtData table-condensed">
